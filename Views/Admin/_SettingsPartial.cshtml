@model IEnumerable<SettingModel>
@if (Model.Count() > 0)
{
    @foreach (var item in Model)
    {
        <div class="form-group">
            <label class="control-label">@item.SettingName</label>
            <input class="form-control" value="@item.SettingValue" />
            <span asp-validation-for="@item.SettingValue" class="text-danger"></span>
        </div>
    }
}
