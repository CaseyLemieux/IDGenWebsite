<div>
    <form method="post" enctype="multipart/form-data" asp-controller="Admin" asp-action="UploadFocus" id="UploadFocusForm">
        <div class="form-group">
            <div class="col-md-10">
                <p>Upload Focus Student Excel File:</p>
                <input type="file" name="focusFiles" multiple />
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-10">
                <input type="submit" value="Upload" />
            </div>
        </div>
    </form>
</div>
