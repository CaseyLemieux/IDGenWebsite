@model IEnumerable<AcademicSessions>
@{
    ViewData["Title"] = "Roster Data";
}
<div class="row">

    <div class="col-md-9">
        @if (Model.Count() > 0)
        {
            <table class="table">
                <thead>
                    <tr>
                        <th>
                            @Html.DisplayNameFor(mode => mode.SessionSourcedId)
                        </th>
                        <th>
                            @Html.DisplayNameFor(mode => mode.Status)
                        </th>
                        <th>
                            @Html.DisplayNameFor(mode => mode.DateLastModified)
                        </th>
                        <th>
                            @Html.DisplayNameFor(mode => mode.Title)
                        </th>
                        <th>
                            @Html.DisplayNameFor(mode => mode.Type)
                        </th>
                        <th>
                            @Html.DisplayNameFor(mode => mode.StartDate)
                        </th>
                        <th>
                            @Html.DisplayNameFor(mode => mode.EndDate)
                        </th>
                        <th>
                            @Html.DisplayNameFor(mode => mode.Parent)
                        </th>
                        <th>
                            @Html.DisplayNameFor(mode => mode.Parent_SourcedId)
                        </th>
                        <th>
                            @Html.DisplayNameFor(mode => mode.SchoolYear)
                        </th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.SessionSourcedId)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Status)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.DateLastModified)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Title)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Type)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.StartDate)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.EndDate)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Parent)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Parent_SourcedId)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.SchoolYear)
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        }
    </div>
</div>