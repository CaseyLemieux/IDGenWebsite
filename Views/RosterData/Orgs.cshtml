@model IEnumerable<Organizations>
@{
    ViewData["Title"] = "Roster Data";
}
<div class="row">
  
    <div class="col-md-9">
        @if (Model.Count() > 0)
        {
            <table class="table">
                <thead>
                    <tr>
                        <th>
                            @Html.DisplayNameFor(mode => mode.OrgSourcedId)
                        </th>
                        <th>
                            @Html.DisplayNameFor(mode => mode.Status)
                        </th>
                        <th>
                            @Html.DisplayNameFor(mode => mode.DateLastModified)
                        </th>
                        <th>
                            @Html.DisplayNameFor(mode => mode.Name)
                        </th>
                        <th>
                            @Html.DisplayNameFor(mode => mode.Type)
                        </th>
                        <th>
                            @Html.DisplayNameFor(mode => mode.Identifier)
                            <i class="bi bi-sort-up"></i>
                        </th>
                        <th>
                            @Html.DisplayNameFor(mode => mode.Parent)
                        </th>
                        <th>
                            @Html.DisplayNameFor(mode => mode.Parent_SourcedId)
                        </th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.OrgSourcedId)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Status)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.DateLastModified)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Name)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Type)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Identifier)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Parent)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Parent_SourcedId)
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        }
    </div>
</div>