@model IEnumerable<Courses>
@{
    ViewData["Title"] = "Roster Data";
}
<div class="row">

    <div class="col-md-9">
        @if (Model.Count() > 0)
        {
            <table class="table">
                <thead>
                    <tr>
                        <th>
                            @Html.DisplayNameFor(mode => mode.CourseSourcedId)
                        </th>
                        <th>
                            @Html.DisplayNameFor(mode => mode.Status)
                        </th>
                        <th>
                            @Html.DisplayNameFor(mode => mode.DateLastModified)
                        </th>
                        <th>
                            @Html.DisplayNameFor(mode => mode.Title)
                        </th>
                        <th>
                            @Html.DisplayNameFor(mode => mode.SchoolYear)
                        </th>
                        <th>
                            @Html.DisplayNameFor(mode => mode.CourseCode)
                        </th>
                        <th>
                            @Html.DisplayNameFor(mode => mode.OrgSourcedId)
                        </th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.CourseSourcedId)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Status)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.DateLastModified)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Title)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.SchoolYear)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.CourseCode)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.OrgSourcedId)
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        }
    </div>
</div>